        <section class="padding-110px-tb xs-padding-60px-tb blog-style1 bg-white builder-bg" >
            <div class="container">
                <!-- section title -->
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12 text-center">
                        <h2 class="section-title-large sm-section-title-medium xs-section-title-large text-dark-speech-blue font-weight-700 alt-font margin-three-bottom xs-margin-fifteen-bottom tz-text">PROPIEDADES RECIENTES</h2>
                        <div class="text-medium width-60 margin-lr-auto md-width-70 sm-width-100 tz-text margin-thirteen-bottom  xs-margin-nineteen-bottom">Estas son algunas de las más recientes propiedades, si desea ver más por favor haga clic en el boton <strong>Ver más propiedades</strong></div>
                    </div>
                </div>
                <!-- end section title -->
                <div class="row">
                    {{#each imageList}}
                    <!-- blog item -->
                    <div class="col-md-4 col-sm-4 col-xs-12 xs-margin-nineteen-bottom">
                        <div class="blog-post">
                            <!-- blog image -->
                            <div class="blog-image">
                                <a href="#"><img class="img100" alt="" src="{{this.src}}" data-img-size="(W)800px X (H)507px"></a>
                            </div>
                            <!-- end blog image -->
                            <!-- blog details -->
                            <div class="post-details bg-gray tz-background-color">
                                <div class="post-author tz-background-color bg-light-blue margin-thirteen-bottom md-margin-eleven-bottom xs-margin-eight-bottom"><a href="#" class="tz-text no-letter-spacing text-white blog-name">Habitaciones: {{this.beds}} &nbsp;| &nbsp;Baños: {{bathrooms}} &nbsp;| &nbsp;</a> <span class="tz-text">Price: {{this.price}}</span></div>
                                <a href="#" class="tz-text text-dark-gray blog-post-title text-medium font-weight-600 margin-five-bottom display-inline-block md-margin-five-bottom">{{this.title}}</a>
                                <div class="text-small tz-text">Zona: {{this.zone}}, Barrio: {{this.neighbor}} <br>Ciudad: {{this.city}}, Colombia</div>
                            </div>
                            <!-- end blog details -->
                        </div>
                    </div>
                    <!-- end blog item -->
                    {{/each}}

                    <!-- view more button -->
                    <div class="col-md-12 col-sm-12 col-xs-12 text-center margin-ten-top xs-no-margin-top">
                        <a href="#properties" class="btn btn-medium btn-circle bg-dark-speech-blue text-white propClone tz-text">VEA TODAS LAS PROPIEDADES</a>
                    </div>
                    <!-- end view more button -->
                </div>
            </div>
        </section>


